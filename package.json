    "angular": "1.7.0",
    "angular-local-storage": "0.5.2",
    "angular-messages": "1.7.0",
    "angular-route": "1.7.0",
    "angular-sanitize": "1.7.0",
    "angular-translate": "2.18.1",
    "angular-translate-interpolation-messageformat": "2.18.1",
    "babel-polyfill": "6.26.0",
    "components-font-awesome": "4.7.0",

{
  "name": "DestinyRaidStatus",
  "version": "2.0.1",
  "description": "Easily check raid status of Destiny player(s)",
  "main": "app/index.html",
  "engines": {
    "node": "6.9.2"
  },
  "scripts": {
    "test": "echo \"No tests\"",
    "dev-chrome": "npm start -s",
    "lint": "eslint . --fix=true",
    "bundle-prod": "webpack --config ./config/webpack.prod.js --hide-modules",
    "bundle-dev": "webpack --config ./config/webpack.dev.js --hide-modules",
    "build-prod": "npm run bundle-prod -s -- --env=prod",
    "build-dev": "npm run bundle-dev -s -- --env=dev --watch",
    "start": "npm run build-dev -s",
    "server-prod": "webpack-dev-server --config ./config/webpack.prod.js --env=prod",
    "server-dev": "webpack-dev-server --config ./config/webpack.dev.js --env=dev",
    "install": "napa"
  },
  "napa": {
    "angular-uuid2": "https://github.com/MBehtemam/angular-uuid"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mulchman/DestinyRaidStatus"
  },
  "author": "Mulchman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mulchman/DestinyRaidStatus/issues"
  },
  "homepage": "https://github.com/Mulchman/DestinyRaidStatus",
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-angularjs-annotate": "0.7.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-es2015": "6.24.1",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "eslint": "3.19.0",
    "exports-loader": "0.6.4",
    "extract-loader": "0.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.7.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.7.1",
    "lodash-webpack-plugin": "0.11.5",
    "napa": "2.3.0",
    "node-sass": "4.9.0",
    "notify-webpack-plugin": "1.0.0",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "request": "2.87.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.17.0",
    "uglify-js": "2.8.29",
    "url-loader": "0.5.9",
